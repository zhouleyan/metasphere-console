{
  "name": "metasphere-console",
  "version": "0.0.1",
  "description": "MetaSphere Console",
  "author": "zhouleyan",
  "private": true,
  "browserslist": [
    "chrome >= 61",
    "firefox >= 60",
    "safari >= 11",
    "edge >= 16"
  ],
  "engines": {
    "node": ">=14.13.1"
  },
  "lint-staged": {
    "src/**/*.less": "stylelint \"src/**/*.less\"",
    "{src,server}/**/*.{js,jsx,ts,tsx}": "eslint --cache"
  },
  "scripts": {
    "start": "concurrently -k --raw \"yarn dev:client\" \"yarn dev:server\"",
    "build": " yarn build:locale && yarn build:client && yarn build:server",
    "serve": "cross-env NODE_ENV=production node server/index.js",
    "serve:dist": "cross-env NODE_ENV=production node server/server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config scripts/webpack.dev.js",
    "dev:server": "cross-env NODE_ENV=development nodemon -w ./server -e js,yaml server/index.js",
    "build:client": "webpack --config scripts/webpack.prod.js",
    "build:server": "webpack --config scripts/webpack.server.js && mv dist/server.js server/server.js",
    "build:locale": "node scripts/merge-locale.js",
    "postinstall": "rimraf node_modules/.cache",
    "prebuild": "rimraf dist",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx {src,server,scripts}",
    "lint:style": "stylelint --fix \"{src,server}/**/*.{css,less}\"",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx {src,server,scripts} && yarn lint:style",
    "tsc": "tsc",
    "plop": "plop --plopfile scripts/plopfile.js",
    "test": "yarn jest"
  },
  "devDependencies": {
    "@babel/cli": "7.17.0",
    "@babel/core": "7.17.2",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-proposal-decorators": "7.17.2",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@babel/register": "7.17.0",
    "@babel/runtime": "7.17.2",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@koa/router": "10.1.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@svgr/webpack": "6.2.1",
    "@types/node": "17.0.17",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "autoprefixer": "10.4.2",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.3",
    "boxen": "5.1.2",
    "chalk": "4.1.2",
    "concurrently": "7.0.0",
    "copy-webpack-plugin": "10.2.4",
    "corejs": "1.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.6.0",
    "css-minimizer-webpack-plugin": "3.4.1",
    "cssnano": "5.0.17",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.0",
    "eslint-plugin-jsdoc": "37.9.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "glob": "7.2.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "js-yaml": "4.1.0",
    "koa": "2.13.4",
    "koa-bodyparser": "4.3.0",
    "koa-compress": "5.1.0",
    "koa-ejs": "4.3.0",
    "koa-mount": "4.0.0",
    "koa-static": "5.0.0",
    "lint-staged": "12.3.3",
    "lodash": "4.17.21",
    "mini-css-extract-plugin": "2.5.3",
    "node-cache": "5.1.2",
    "node-fetch": "3.2.0",
    "node-sass": "7.0.1",
    "nodemon": "2.0.15",
    "plop": "3.0.5",
    "postcss": "8.4.6",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "6.2.1",
    "prettier": "2.5.1",
